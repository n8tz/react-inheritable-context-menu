{
  "name": "react-inheritable-contextmenu",
  "version": "1.3.1",
  "license": "MIT",
  "author": "Nathan Braun <caipilabs@gmail.com>",
  "main": "./dist/Comp.js",
  "wpInherit": {
    "default": {
      "rootFolder": "src",
      "basedOn": "Component",
      "vars": {
        "externals": "^(react|is)",
        "webpackPatch": {
        }
      },
      "extend": [
        "wpi-react-hmr-ssr"
      ]
    },
    "samples": {
      "rootFolder": "src",
      "basedOn": "ComponentSamples",
      "vars": {
        "webpackPatch": {}
      },
      "extend": [
        "wpi-react-hmr-ssr"
      ]
    }
  },
  "scripts": {
    "build": "wpi && wpi :samples",
    "dev": "wpi-dev-server :samples --hot",
    "runBrowser": "start http://localhost:9501"
  },
  "peerDependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "dependencies": {
    "is": "^3.2.1"
  },
  "devDependencies": {
    "wpi-react-hmr-ssr": "^0.2.0",
    "webpack-inherit": "^1.3.10"
  }
}

{
  "name": "react-inheritable-contextmenu",
  "version": "1.4.7",
  "description": "Simple Context menu component for react showing all inherited parents menu with SSR compatibility.",
  "license": "MIT",
  "author": "Nathan Braun <n8tz.js@gmail.com>",
  "main": "./dist/Comp.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/n8tz/react-inheritable-contextmenu"
  },
  "wpInherit": {
    "default": {
      "rootFolder": "src",
      "basedOn": "Component",
      "vars": {
        "externals": "^(react|is)",
        "webpackPatch": {}
      },
      "extend": [
        "wpi-react-hmr-ssr"
      ]
    },
    "samples": {
      "rootFolder": "src",
      "basedOn": "ComponentSamples",
      "vars": {
        "webpackPatch": {
          "output": {}
        }
      },
      "extend": [
        "wpi-react-hmr-ssr"
      ]
    }
  },
  "scripts": {
    "build": "wpi && wpi :samples",
    "dev": "wpi-dev-server :samples --hot",
    "runBrowser": "start http://localhost:9501"
  },
  "peerDependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "dependencies": {
    "classlist-polyfill": "^1.2.0",
    "is": "^3.2.1"
  },
  "devDependencies": {
    "wpi-react-hmr-ssr": "^0.2.1",
    "webpack-inherit": "^1.3.10"
  }
}
